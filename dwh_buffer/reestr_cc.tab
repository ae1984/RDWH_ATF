  contact_id         NUMBER,
  upload_dttm        DATE,
  campaign_code      VARCHAR2(20 CHAR),
  comment_ext        VARCHAR2(500 CHAR),
  channel            VARCHAR2(20 CHAR),
  full_name          VARCHAR2(500 CHAR),
  filial_name        VARCHAR2(200 CHAR),
  contact            VARCHAR2(12 CHAR),
  iin                VARCHAR2(12 CHAR),
  cli_code           VARCHAR2(30 CHAR),
  segm               VARCHAR2(50 CHAR),
  offer_product_name VARCHAR2(200 CHAR),
  offer_term         NUMBER,
  offer_int_rate     NUMBER,
  offer_comm_rate    NUMBER,
  offer_limit        NUMBER,
  offer_close_dttm   DATE
)
tablespace DWH_BUFFER
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 128K
    next 128K
    minextents 1
    maxextents unlimited
