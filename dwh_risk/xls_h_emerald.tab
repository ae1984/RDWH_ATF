create table DWH_RISK.XLS_H_EMERALD
(
  company_name VARCHAR2(500),
  bin          VARCHAR2(500),
  type         VARCHAR2(500),
  fil          VARCHAR2(500),
  emp_cnt      VARCHAR2(500),
  fot          VARCHAR2(500)
)
