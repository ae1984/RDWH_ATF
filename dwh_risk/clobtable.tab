create table DWH_RISK.CLOBTABLE
(
  id    NUMBER,
  name  VARCHAR2(30),
  value CLOB
)
tablespace DWH_RISK
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );
grant select on DWH_RISK.CLOBTABLE to AIBEK_BE;
grant select, insert, update, delete on DWH_RISK.CLOBTABLE to DWH_RISK_FULL_ROLE;
grant select on DWH_RISK.CLOBTABLE to DWH_RISK_READ_ROLE;
grant select on DWH_RISK.CLOBTABLE to HEAD_DENIS_PL;
grant select on DWH_RISK.CLOBTABLE to KRISTINA_KO;
grant select on DWH_RISK.CLOBTABLE to PROCESS_RISK;

